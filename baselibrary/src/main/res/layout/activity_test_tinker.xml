<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.activities.TestTinkerActivity">

    <TextView
        android:id="@+id/tv_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="这是TestTinkerActivity"
        android:textSize="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="15dp" />


    <TextView
        android:id="@+id/tv_load"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Load Path"
        android:textSize="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_title"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="15dp"
        android:layout_marginStart="30dp" />

    <TextView
        android:id="@+id/tv_clear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Clear Path"
        android:textSize="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_load"
        app:layout_constraintStart_toStartOf="@+id/tv_load"
        android:layout_marginTop="15dp" />

    <TextView
        android:id="@+id/tv_toast"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Toast"
        android:textSize="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_clear"
        app:layout_constraintStart_toStartOf="@+id/tv_clear"
        android:layout_marginTop="15dp" />

    <TextView
        android:id="@+id/tv_show"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="show info"
        android:textSize="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_toast"
        app:layout_constraintStart_toStartOf="@+id/tv_toast"
        android:layout_marginTop="15dp" />

    <TextView
        android:id="@+id/tv_killself"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="killself"
        android:textSize="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tv_show"
        app:layout_constraintStart_toStartOf="@+id/tv_show"
        android:layout_marginTop="15dp" />


</androidx.constraintlayout.widget.ConstraintLayout>